}
swiper {
  height: 750rpx;
}
.slick-slide:after {
  content: '';
  display: inline-block;
  height: 100%;
  vertical-align: middle;
}
.slick-slide {
  display: -webkit-box;
  display: -moz-box;
  display: -ms-flexbox;
  display: -webkit-flex;
  display: flex;
  -webkit-align-items: center;
          align-items: center;
  -webkit-justify-content: center;
          justify-content: center;
}
.slick-slide image {
  width: 100%;
  height: auto;
}
