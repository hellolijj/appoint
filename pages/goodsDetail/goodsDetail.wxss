.goods-seckill{
  height: 52px;
  background-color: #f31e4a;
  overflow: hidden;
}
.goods-seckill-left{
  display: inline-block;
  width: 468rpx;
  height: 100%;
  padding-left: 23.4rpx;
  position: relative;
}
.goods-seckill-right{
  float: right;
  width: 234rpx;
  height: 100%;
  background-color: #feef72;
  padding-left: 23.4rpx;
  position: relative;
  text-align: center;
}
.goods-seckill-left:after{
  content: '';
  width: 0;
  height: 0;
  border-top: 131.04rpx solid #f31e4a;
  border-right: 23.4rpx solid transparent;
  vertical-align: top;
  position: absolute;
  right: -21.06rpx;
  top: 0;
  z-index: 1;
}
.goods-seckill-right.seckill-end{
  background-color: #988d8f;
}
.goods-seckill-right.seckill-end .countdown > label{
