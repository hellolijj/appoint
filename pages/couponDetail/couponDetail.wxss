page {
  height: 100%;
}
#couponDetail{
  height: 100%;
  background-color: #eee;
}
.coupon-detail-wrap {
  display: none;
  margin: 23rpx 23rpx 0;
}
.coupon-detail-wrap.show {
  display: block;
}
.coupon-detail-wrap .top-section {
  position: relative;
  width: 100%;
  height: 104rpx;
  background: #fff;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  color: #8b8b8b;
  box-shadow: 0px -5px 5px -5px rgba(0, 0, 0, 0.2),
              5px 0px 5px -5px rgba(0, 0 , 0, 0.2),
              -5px 0px 5px -5px rgba(0, 0 , 0, 0.2);
  z-index: 1;
}
.coupon-detail-wrap .top-section::after {
  content: '';
  position: absolute;
  bottom: 0;
  left: 9rpx;
  width: calc(100% - 20rpx);
}
.coupon-detail-wrap .top-section .logo {
  position: absolute;
  top: 22rpx;
  left: 36rpx;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}
.coupon-detail-wrap .top-section .name {
  position: absolute;
  top: 38rpx;
  left: 116rpx;
  height: 30rpx;
  line-height: 30rpx;
  font-size: 28rpx;
}
.coupon-detail-wrap .top-section .share {
  position: absolute;
  top: 24rpx;
  right: 36rpx;
  width: 48rpx;
  height: 48rpx;
  color: #8a8b8c;
  font-size: 48rpx;
  line-height: 48rpx;
}
.coupon-detail-wrap .top-section .share .share-btn {
  display: block;
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  opacity: 0;
}
.coupon-detail-wrap  .top-section .division-line {
  position: absolute;
  top: 104rpx;
  width: 100%;
  border-top: 1px dashed #dbdbdb;
}
.coupon-detail-wrap  .top-section .division-line .left-circle{
  position:absolute;
  top:-5px;
  left:-5px;
  width:10px;
  height:10px;
  border-radius:50%;
  background:#eee;
}
.coupon-detail-wrap .division-line .right-circle{
  position:absolute;
  top:-5px;
  right:-5px;
  width:10px;
  height:10px;
  border-radius:50%;
  background:#eee;
}
.coupon-detail-wrap .middle-section {
  position: relative;
  background: #fff;
  box-shadow: 5px 0px 5px -5px rgba(0, 0 , 0, 0.2), -5px 0px 5px -5px rgba(0, 0 , 0, 0.2);
}
.coupon-detail-wrap .middle-section .title {
  padding-top: 57rpx;
  line-height: 59rpx;
  font-size: 59rpx;
  text-align: center;
  font-weight: 400;
}
.coupon-detail-wrap .middle-section .sub-title {
  margin-top: 31rpx;
  line-height: 40rpx;
  font-size: 40rpx;
  text-align: center;
}
.coupon-detail-wrap .middle-section .qrcode {
  position: absolute;
  top: 74rpx;
  right: 36rpx;
  color: #8a8b8c;
  font-size: 48rpx;
  line-height: 48rpx;
}
.coupon-detail-wrap .middle-section .function-btn {
  margin: 57rpx auto 0;
  width: 340rpx;
  height: 69rpx;
  line-height: 69rpx;
  background: #ff4d64;
  border-radius: 4px;
  font-size: 33rpx;
  text-align: center;
  color: #fff;
}
.coupon-detail-wrap .middle-section .function-btn.disabled {
  background: #ddd!important;
}
.coupon-detail-wrap .middle-section .detail-item {
  margin: 0 36rpx;
  padding-bottom: 36rpx;
  line-height: 28rpx;
  font-size: 28rpx;
}
.middle-section .detail-item .item-name {
  display: inline-block;
  vertical-align: top;
}
.coupon-detail-wrap .middle-section .detail-item .item-content {
  display: inline-block;
  color: #8a8b8c;
  width: calc(100% - 70px);
  vertical-align: top;
  overflow: hidden;
}
.coupon-detail-wrap .middle-section .detail-item.condition {
  margin-top: 46rpx;
}
.coupon-detail-wrap .middle-section .detail-item.time {
  padding-bottom: 59rpx;
}
.coupon-detail-wrap .middle-section .detail-item.time .item-content {
  line-height: 40rpx;
}
.coupon-detail-wrap .middle-section .detail-item.time .item-content > view  {
  display: inline-block;
  margin-right: 10rpx;
}
.coupon-detail-wrap .bottom-section {
  background-color: #fff;
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  border-top: 1px dashed #dbdbdb;
  box-shadow: 0px 0px 5px 0px rgba(0, 0 , 0, 0.2);
}
.coupon-detail-wrap .bottom-section .detail-item {
  margin: 0 36rpx;
  padding: 40rpx 0;
  border-bottom: 1px solid #dbdbdb;
  line-height: 28rpx;
  font-size: 28rpx;
}
.coupon-detail-wrap .bottom-section .detail-item .item-name {
  display: inline-block;
  vertical-align: top;
  color: #333;
}
.coupon-detail-wrap .bottom-section .detail-item .item-content {
  display: inline-block;
  width: calc(100% - 150rpx);
  color: #8b8b8b;
  vertical-align: top;
}
.coupon-detail-wrap .bottom-section .detail-item .item-right {
  position:relative;
  padding:20rpx 0;
  top:-20rpx;
  float: right;
  color: #8b8b8b;
  font-size: 24rpx;
}
.coupon-detail-wrap .bottom-section .detail-item .item-right text {
  vertical-align: middle;
}
.coupon-detail-wrap .bottom-section .detail-item .item-right label {
  vertical-align: sub;
}
.coupon-detail-wrap .bottom-section .goto-homepage {
  position: relative;
  padding: 40rpx 36rpx;
  line-height: 28rpx;
  font-size: 28rpx;
}
.coupon-detail-wrap .bottom-section .goto-homepage .icon-rightarrow {
  position: absolute;
  top: 35rpx;
  right: 36rpx;
  color: #8b8b8b;
  font-size: 24rpx;
}
.zc-toast-bg {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  display: none;
  background: rgba(1,1,1,0.5);
  z-index: 999;
  /*opacity: 0;*/
  /*transition: opacity 3s linear;*/
}
.zc-toast-bg.show {
  /*opacity: 1;*/
  display: block;
}
.zc-toast {
  position: absolute;
  top: 350rpx;
  left: 50%;
  margin-left: -200rpx;
  width: 400rpx;
  height: 165rpx;
  background: #fff;
  border-radius: 6px;
}
.zc-toast .icon {
  position: absolute;
  top: 38rpx;
  left: 78rpx;
}
.zc-toast .title {
  position: absolute;
  top: 43rpx;
  left: 140rpx;
  height: 35rpx;
  line-height: 35rpx;
  color: #333;
  font-size: 35rpx;
}
.zc-toast.receive-success .content {
  position: absolute;
  top: 95rpx;
  left: 0;
  width: 100%;
  height: 26rpx;
  line-height: 26rpx;
  color: #666;
  font-size: 26rpx;
  text-align: center;
}
.zc-toast.recharge-success .content {
  position: absolute;
  top: 95rpx;
  left: 125rpx;
  height: 26rpx;
  line-height: 26rpx;
  color: #666;
  font-size: 26rpx;
}
.coupon-verify-wrap {
  display: none;
  margin: 45rpx 30rpx 0;
}
.coupon-verify-wrap.show {
  display: block;
}
.coupon-verify-wrap .top-section {
  position: relative;
  width: 100%;
  height: 320rpx;
  background: #fff;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  color: #8b8b8b;
  box-shadow: 0px -5px 5px -5px rgba(0, 0, 0, 0.2),
              5px 0px 5px -5px rgba(0, 0 , 0, 0.2),
              -5px 0px 5px -5px rgba(0, 0 , 0, 0.2);
}
.coupon-verify-wrap .top-section .cancel {
  position: absolute;
  top: 45rpx;
  left: 45rpx;
  color: #333;
}
.coupon-verify-wrap .top-section .name {
  padding-top: 140rpx;
  text-align: center;
  color: #666;
  font-size: 35rpx;
}
.coupon-verify-wrap .top-section .title {
  padding-top: 25rpx;
  text-align: center;
  color:#333;
  font-size: 60rpx;
}
.coupon-verify-wrap .division-line {
  position: relative;
  width: 100%;
  border-top: 1px dotted #dbdbdb;
}
.coupon-verify-wrap .division-line .left-circle{
  position:absolute;
  top:-5px;
  left:-5px;
  width:10px;
  height:10px;
  border-radius:50%;
  background:#eee;
}
.coupon-verify-wrap .division-line .right-circle{
  position:absolute;
  top:-5px;
  right:-5px;
  width:10px;
  height:10px;
  border-radius:50%;
  background:#eee;
}
.coupon-verify-wrap .bottom-section {
  background: #fff;
  box-shadow: 5px 0px 5px -5px rgba(0, 0 , 0, 0.2), -5px 0px 5px -5px rgba(0, 0 , 0, 0.2);
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  z-index: 1;
  text-align: center;
}
.coupon-verify-wrap .bottom-section::after {
  content:'';
  position:absolute;
  bottom:0;
  width:100%;
  height:22px;
  border-bottom-left-radius:4px;
  border-bottom-right-radius:4px;
  box-shadow:0px 0px 5px 0px rgba(0, 0 , 0, 0.2);
  z-index:-1;
}
.coupon-verify-wrap .bottom-section .verify-block {
  height: 550rpx;
}
.coupon-verify-wrap .bottom-section .success-block {
  height: 350rpx;
}
.coupon-verify-wrap .bottom-section .qrcode {
  display: inline-block;
  width: 280rpx;
  height: 280rpx;
  padding-top: 50rpx;
}
.coupon-verify-wrap .bottom-section .code {
  margin-top:15rpx;
  color:#333;
  font-size:35rpx;
}
.coupon-verify-wrap .bottom-section .tip {
  margin-top:18rpx;
  font-size:28rpx;
  color:#666;
}
.coupon-verify-wrap .bottom-section .success-tip {
  padding-top: 75rpx;
}
.coupon-verify-wrap .bottom-section .success-tip-icon {
  display: inline-block;
  vertical-align: middle;
}
.coupon-verify-wrap .bottom-section .success-tip-text {
  display: inline-block;
  height: 24px;
  line-height: 24px;
  font-size: 20px;
  margin-left: 8px;
  vertical-align: middle;
}
.coupon-verify-wrap .bottom-section .back-tip {
  margin-top: 28rpx;
  font-size: 28rpx;
  color:#666;
}
