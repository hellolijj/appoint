/**index.wxss**/
.index-banner image{
  width: 100%;
  min-height: 300rpx;
}
.index-exam-h1{
  padding: 26rpx 30rpx 16rpx;
  background: #fff;
  border-bottom: 1px solid #eeeeeb;
  font-size: 34rpx;
}
.index-exam-h1 .index-exam-span{
  display: inline-block;
  font-size:26rpx;
  color:#a9a9a9;
}
.index-exam-inlets{
  margin-bottom: 10rpx;
  background: #fff;
}
.index-exam-inlets .index-exam-inlets-mk{
  position: relative;
  height: 180rpx;
  padding: 48rpx 0 0 175rpx; 
  border-right: 1px solid #eeeeeb;
  border-bottom: 1px solid #eeeeeb;
  font-size: 30rpx;
  color:#666;
}
.index-exam-inlets .index-exam-inlets-mk:after{
  position: absolute;
  left: 46rpx;
  top: 50%;
  transform: translate3d(0, -50%, 0px);
  -webkit-transform: translate3d(0, -50%, 0px);
  width: 110rpx;
  height: 110rpx;
  content: '';
  border-radius: 55rpx;
  background: #24c5d9 url('data:image/png;base64,') no-repeat center/51rpx 49rpx;
}
.index-exam-inlets .index-exam-inlets-mk .small{
  padding: 17rpx 0 0;
  font-size: 22rpx;
  color: #999;
}
.index-exam-inlets .index-exam-inlets-sc,.index-exam-inlets .index-exam-inlets-ct{
  position: relative;
  height: 90rpx;
  padding: 0 0 0 160rpx;
  border-bottom: 1px solid #eeeeeb;
  font-size: 22rpx;
  color: #999;
  line-height: 90rpx;
}
.index-exam-inlets .index-exam-inlets-sc view,.index-exam-inlets .index-exam-inlets-ct view{
  display: inline-block;
}
.index-exam-inlets .index-exam-inlets-sc:after,.index-exam-inlets .index-exam-inlets-ct:after{
  position: absolute;
  left: 103rpx;
  top: 50%;
  content: '';
  transform: translate3d(0, -50%, 0px);
  -webkit-transform: translate3d(0, -50%, 0px);
}
.index-exam-inlets .index-exam-inlets-sc:after{
  width: 40rpx;
  height: 38rpx;
  background: url('data:image/png;base64,') no-repeat center/40rpx 38rpx;
}
.index-exam-inlets .index-exam-inlets-ct:after{
  width: 40rpx;
  height: 38rpx;
  background: url('data:image/png;base64,') no-repeat center/40rpx 38rpx;
}
.index-exam-inlets-list{
  height: 110rpx;
  padding:24rpx 0 0 0;
  border-right: 1px solid #eeeeeb;
  text-align: center;
  font-size: 22rpx;
  color: #999;
}
.index-exam-inlets-list .text{
  padding: 4rpx 0 16rpx;
}
.index-exam-inlets-list .icon-index-zx{
  display: inline-block;
  width: 32rpx;
  height: 29rpx;
  background: url('data:image/png;base64,') no-repeat center/32rpx 29rpx;
}
.index-exam-inlets-list .icon-index-zj{
  display: inline-block;
  width: 32rpx;
  height: 29rpx;
  background: url('data:image/png;base64,') no-repeat center/32rpx 29rpx;

}
.index-exam-inlets-list .icon-index-sj{
  display: inline-block;
  width: 32rpx;
  height: 29rpx;
  background: url('data:image/png;base64,') no-repeat center/32rpx 27rpx;
}
.index-exam-inlets-list .icon-index-sx{
  display: inline-block;
  width: 32rpx;
  height: 29rpx;
  background: url('data:image/png;base64,') no-repeat center/32rpx 26rpx;
}
.container{
  min-height: 100rpx;
  margin: 0;
}