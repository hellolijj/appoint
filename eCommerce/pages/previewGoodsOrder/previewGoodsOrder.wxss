page {
  padding-bottom: 125rpx;
  font-size: 29rpx;
  color: #444;
  background-color: #f2f2f2;
}
.page-top-nav {
  position: static;
  margin: 0;
}
.preview-goods-order-section {
  padding: 34rpx 36rpx;
  margin-bottom: 11rpx;
  background: #fdfdfd;
  overflow: hidden;
}
.preview-goods-order-list {
  padding-top: 0;
  padding-bottom: 0;
}
.preview-goods-order-item {
  position: relative;
  padding-top: 17rpx;
  padding-bottom: 17rpx;
  border-bottom: 1px solid #e8e8e8;
}
.preview-goods-order-item:last-child {
  border-bottom: none;
}
.preview-address-options {
  position: absolute;
  top: 0;
  right: 0;
  font-size: 31rpx;
  text-align: right;
  color: #aaa;
}
.preview-goods-cover, .preview-goods-content {
  display: inline-block;
  vertical-align: top;
}
.preview-goods-cover {
  width: 200rpx;
  height: 200rpx;
}
.preview-goods-content {
  position: relative;
  margin-left: 18rpx;
  width: calc(100% - 218rpx);
  height: 200rpx;
}
.preview-goods-title {
  margin-top: 18rpx;
  font-size: 33rpx;
  color: #535458;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.preview-goods-model {
  margin-top: 20rpx;
  color: #9b9b9b;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.preview-quantity-section {
  position:absolute;
  bottom: 0;
  left: 0;
  width: 100%;
}
.preview-goods-price {
  display: inline-block;
  margin-left: 2px;
  vertical-align: middle;
  line-height: 25px;
  font-size: 33rpx;
  color: #ff7100;
}

textarea {
  width: auto;
  height: 50px;
  padding: 10px;
  border: 1px solid #eee;
  border-radius: 4px;
  background-color: #e9e7e8;
}
.delivery-way-radio {
  margin-right: 15px;
}

.preview-section-title {
  margin-bottom: 8px;
}

.buy-goods-count{
  display:inline-block;
  width: 25px;
  height: 22px;
  line-height: 22px;
  margin: 0;
  box-sizing: content-box;
  font-size: 33rpx;
  text-align: center;
  vertical-align: middle;
  border: none;
}
.quantity {
  position: absolute;
  right: 0;
}
.quantity .minus, .quantity .plus {
  width: 40rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-weight: normal;
  background-color: #ff7100;
  border-color: #ff7100;
  border-radius: 50%;
}
.quantity .minus {
  background-color: #fff;
  border-color: #9b9b9b;
}
.quantity .plus::before, .quantity .minus::before {
  width: 26rpx;
  height: 5rpx;
}
.quantity .plus::after {
  width: 5rpx;
  height: 26rpx;
}
.quantity .plus::after, .quantity .plus::before {
  background-color: #fff;
}
.quantity .minus::before, .quantity .minus.disabled::before {
  background-color: #9b9b9b;
}
.quantity input.buy-goods-count {
  margin: 0 2px;
  width: 35px;
  background-color: #f5f5f5;
  border-radius: 2px;
}
.quantity .response-area {
  top: 0;
  left: -3px;
  width: 60rpx;
  height: 60rpx;
}
.quantity .response-area-plus {
  left: auto;
  right: -3px;
}
.select-discount-title {
  font-size: 29rpx;
  line-height: 24px;
  color: #818181;
}
.discount-way .select-discount-title {
  padding-top:64rpx;
  position:relative;
  z-index:2;
}
.bottom-nav {
  line-height: 94rpx;
  border-top:1px solid #eee;
  font-size: 0;
  background-color: #fff;
}
.preview-goods-total-price {
  display: inline-block;
  width: calc(100% - 234rpx);
  font-size: 33rpx;
  vertical-align:middle;
  color: #666;
  text-align: left;
  text-indent: 36rpx;
}
.bottom-nav form {
  float: right;
  width: 234rpx;
}
.bottom-nav button {
  float: right;
  width: 100%;
  font-size: 33rpx;
  border-radius: 0;
  color: #fff;
  background-color: #ff7100;
  line-height: inherit;
}
.icon-location, .icon-phone {
  font-size: 42rpx;
  vertical-align: sub;
  margin-right: 8px;
}
.price-count {
  color: #ff7100;
}
.use-balance-option {
  line-height: 32px;
  overflow: hidden;
}
switch {
  margin-right: -4px;
}
.group-buy{
  overflow: hidden;
  position: relative;
}
.group-buy ::after{
  content: '';
  background: -webkit-gradient(linear, 0 0, right 0, from(#ff7100), to(#ffc114));
  height: 120rpx;
  width: 120rpx;
  display: block;
  text-align: center;
  line-height: 120rpx;
  transform: rotate(45deg);
  position: absolute;
  left: -60rpx;
  top: -60rpx;
  z-index: 9;
}

.group-buy ::before{
  content: '拼团';
  color: #fff;
  width: 120rpx;
  display: block;
  font-size: 26rpx;
  position: absolute;
  left: -8rpx;
  top: -8rpx;
  z-index: 10;
  transform: rotate(-45deg);
}

/* exchangeDialog */
.thumb {
  float: left;
  width: 200rpx;
  height: 130rpx;
  margin: 4.68rpx 23.4rpx 0 23.4rpx;
  position: relative;
}
.thumb .thumb-wrap {
  width: 200rpx;
  height: 200rpx;
  position: absolute;
  bottom: 0;
  left: 0;
  border: 1px solid #dedede;
  border-radius: 12rpx;
  background-color: #fff;
  overflow: hidden;
}
.thumb .thumb-wrap image {
  width: 100%;
  height: 100%;
}
.pay-goods-title,
.pay-goods-price {
  font-size: 32.76rpx;
  padding-right: 50px;
  line-height: 46.8rpx;
}
.pay-checked-text {
  font-size: 32.76rpx;
  line-height: 46.8rpx;
}
.pay-goods-baseinfo {
  min-height: 135rpx;
}
.pay-current-price {
  color: #f60;
}
.pay-current-price text {
  vertical-align: middle;
  font-size: 18px;
}
.pay-goods-models {
  padding: 0 10px;
  margin: -10px 0;
  max-height: 300px;
  overflow: scroll;
}
.pay-goods-models .pills-list {
  display: inline-block;
  vertical-align: middle;
}
.model-title {
  display: block;
  vertical-align: middle;
  margin-bottom: 5px;
}
.pay-goods-models > view {
  margin: 0;
  padding: 12px 0;
  border-bottom: 1px solid #eee;
}
.pay-goods-models > view:last-child {
  border-bottom: none;
}
.pills-list label {
  color: #353535;
  margin-bottom: 2px;
  margin-top: 2px;
  border: none;
  background-color: #f5f5f5;
  border-radius: 8px;
}
.pills-list label.select {
  position: relative;
  color: #fff;
  background-color: #ff7100;
}
.pills-list label.select:after {
  content: '';
  position: absolute;
  bottom: 0;
  right: 0;
  width: 12px;
  height: 12px;
}
.addToShoppingCart{
  background-color: #f5a623;
}
.buy-goods-directly.disabled,
.addToShoppingCart.disabled{
  background-color: #888;
}
.page-dialog-wrap .minus,
.page-dialog-wrap .plus {
  position: relative;
  display: inline-block;
  font-size: 16px;
  outline: 0!important;
  text-indent: -9999px;
  overflow: hidden;
  vertical-align: middle;
  width: 25px;
  height: 25px;
  line-height: 25px;
  font-weight: normal;
  background-color: #f1f1f1;
  color: #878787;
  border-radius: 2px;
  border: none;
}
.page-dialog-wrap .minus.disabled,
.page-dialog-wrap .plus.disabled {
  background-color: #f9f9f9;
  color: #cacaca;
}
.page-dialog-wrap .minus:before,
.page-dialog-wrap .plus:before,
.page-dialog-wrap .plus:after {
  position:absolute;
  width:8px;
  height:2px;
  top:0;
  left:0;
  right:0;
  margin:auto;
  background-color:#878787;
  bottom:0;
  content:'';
}
.page-dialog-wrap .plus:after {
  width:2px;
  height:8px;
}
.page-dialog-wrap .minus.disabled:before,
.page-dialog-wrap .plus.disabled:before,
.page-dialog-wrap .plus.disabled:after {
  background-color: #cacaca;
}
.page-dialog-wrap .page-dialog-close {
  display: block;
  width: 46.8rpx;
  height: 46.8rpx;
  border: 1px solid #67666f;
  color: #67666f;
  text-align: center;
  line-height: 46.8rpx;
  border-radius: 50%;
  font-size: 37.44rpx;
  top: 23.4rpx;
  right: 23.4rpx;
}
.page-dialog-wrap .quantity input{
  border:none;
  height:25px;
  background-color:#f1f1f1;
  padding:0;
  margin:0 1px;
  border-radius:0;
  width:30px;
  min-height:25px;
  line-height:25px;
  font-size:14px;
}
.page-dialog-wrap .confirm-goods{
  color:#fff;
  border-color:#ff7100;
  background-color:#ff7100;
  border-radius:0;
  margin-top:50rpx;
}
.page-dialog-wrap .gift-text {
  display: inline-block;
  margin-left: 10px;
  border: 1px solid #ff7100;
  color:#ff7100;
  line-height:30rpx;
}
.preview-goods-order-list .gift-text {
  display: inline-block;
  border: 1px solid #ff7100;
  color:#ff7100;
  line-height:30rpx;
}
.page-dialog-wrap .gift-text {
  display: inline-block;
  margin-left: 10px;
  border: 1px solid #ff7100;
  color:#ff7100;
  line-height:30rpx;
}
.discount-way {
  margin-bottom:1px;
  position: relative;
}
.discount-way .preview-section-title {
  position: absolute;
}
.discount-way .icon-rightarrow {
  position:absolute;
  right:60rpx;
  font-size:28rpx;
  color:#aaa;
  top:40rpx;
}